<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.PerfilCostureiraActivity">


    <androidx.cardview.widget.CardView
        android:id="@+id/activity_perfil_costureira_cardView"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginTop="32dp"
        app:cardCornerRadius="250dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/activity_perfil_costureira_imageView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/background_spinner" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/activity_perfil_costureira_nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:text="Nome"
        android:textColor="@color/azul"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_perfil_costureira_cardView" />

    <TextView
        android:id="@+id/activity_perfil_costureira_txt_nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="8dp"
        android:paddingTop="4dp"
        android:paddingBottom="2dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/activity_perfil_costureira_nome"
        app:layout_constraintTop_toBottomOf="@+id/activity_perfil_costureira_nome" />

    <RelativeLayout
        android:id="@+id/activity_perfil_costureira_linha_nome"
        android:layout_width="0dp"
        android:layout_height="0.8dp"
        android:background="@drawable/linha_separacao"
        android:backgroundTint="@color/azul_dark"
        android:foregroundTint="@color/azul"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/activity_perfil_costureira_txt_nome"
        app:layout_constraintTop_toBottomOf="@+id/activity_perfil_costureira_txt_nome" />

